{
  "name": "onlie-shop-festival",
  "version": "1.0.0",
  "description": "Online Shop API is an online shopping festival.",
  "main": "./src/index.ts",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "rm -rf built && tsc",
    "docker:dev": "docker-compose -f docker-compose.dev.yml --env-file .env",
    "infra:up": "yarn docker:dev up -d",
    "infra:down": "yarn docker:dev down",
    "migrate": "dotenv -e .env -- prisma migrate dev",
    "migrate:reset": "dotenv -e .env -- prisma migrate reset",
    "prisma:seed": "node ./prisma/seed.js",
    "pretest": "eslint .",
    "start": "yarn build && dotenv -e .env -- node ./built/src/server/index.js",
    "start:dev": "yarn swagger:autogen && yarn migrate && yarn prisma:seed && yarn start",
    "swagger:autogen": "node swagger.js",
    "test:docker:dev": "docker-compose -f docker-compose.test.yml --env-file .env",
    "test:infra:up": "yarn test:docker:dev up -d",
    "test:infra:down": "yarn test:docker:dev down",
    "test:integration": "dotenv -e .env.test -- jest --config jest.integration.js --passWithNoTests --run-in-band",
    "test:integration:sh": "./bin/docker-compose-test-up.sh && dotenv -e .env.test -- jest --config jest.integration.js --passWithNoTests --run-in-band",
    "test:migrate": "dotenv -e .env.test -- prisma migrate dev"
  },
  "author": "sagar@snappymob.com",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^3.9.1",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "config": "^3.3.7",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.16.4",
    "express-async-errors": "^3.1.1",
    "express-winston": "^4.2.0",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "swagger-autogen": "^2.11.2",
    "swagger-ui-express": "^4.1.6",
    "typescript": "^4.5.5",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/config": "^0.0.41",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/swagger-ui-express": "^4.1.3",
    "artillery": "^1.7.7",
    "dotenv-cli": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^27.5.1",
    "mocha": "^6.1.4",
    "nodemon": "^2.0.15",
    "prettier": "^2.3.2",
    "prisma": "^3.9.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0"
  },
  "engines": {
    "node": ">=8.6.0"
  }
}
